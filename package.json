{
    "name": "neuralnetworkfromscratch-typescript",
    "version": "1.0.0",
    "description": "https://nnfs.io typescript implementation",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "p": "node ./node_modules/.bin/ts-node ./src/index.ts 2> out",
        "pi": "node --inspect ./node_modules/.bin/ts-node ./src/index.ts 2> out",
        "pib": "node --inspect-brk ./node_modules/.bin/ts-node ./src/index.ts 2> out",
        "t": "node --expose-gc ./node_modules/.bin/jest",
        "tib": "node --inspect-brk --expose-gc ./node_modules/.bin/jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bryall/NeuralNetworkFromScratch-Typescript.git"
    },
