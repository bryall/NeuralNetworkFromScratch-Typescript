        "url": "https://github.com/bryall/NeuralNetworkFromScratch-Typescript/issues"
    },
    "homepage": "https://github.com/bryall/NeuralNetworkFromScratch-Typescript#readme",
    "dependencies": {
        "@types/jest": "^25.1.2",
        "@types/node": "^13.1.6",
        "jest": "^25.1.0",
        "ts-jest": "^25.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.8.2",
        "eslint": "^7.14.0",
        "prettier": "2.2.0",
        "typescript": "^3.9.7"
    }
